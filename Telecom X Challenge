      "source": [
        "##Data Analysis"
      ],
      "metadata": {
        "id": "eB_TezA-tNls"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Calculate the Daily Spending for Each Customer\n",
        "data = data.drop(\"Daily_Expense\", errors='ignore')\n",
        "data[\"Daily_Expense\"] = ((data[\"Account_TotalCharges\"] / data[\"Customer_Tenure\"]) / 30).round(2) #calcule o gasto diario dividindo o gasto total pelo tempo de contrato\n",
        "\n",
        "# Inserting the Column in the DataFrame\n",
        "data.insert(19, \"Daily_Expense\", data.pop(\"Daily_Expense\"))\n",
        "data.head()"
      ],
