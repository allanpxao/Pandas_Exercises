      "source": [
        "# Group by Churn and Tenure\n",
        "data_group = data.groupby([\"Customer_Tenure\", \"Churn\"]).size().unstack(fill_value=0)\n",
        "\n",
        "# Calculate Churn by Tenure Contract\n",
        "data_group[\"Churn_Rate\"] = (data_group.get('Yes', 0) / data_group.sum(axis=1)) * 100\n",
        "data_group"
