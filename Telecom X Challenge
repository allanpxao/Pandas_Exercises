        "file_path = '/content/TelecomX_Data.json'\n",
        "\n",
        "# 1. Open and load the JSON data from the file\n",
        "with open(file_path, 'r') as f:\n",
        "    data = json.load(f)\n",
        "\n",
        "# 2. Now, normalize the loaded JSON data\n",
        "#    The 'data' variable now holds the content of your file\n",
