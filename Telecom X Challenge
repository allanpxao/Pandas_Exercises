    {
      "cell_type": "code",
      "source": [
        "# Agrupa por forma de pagamento e cancelamento, conta os clientes\n",
        "data_gender = data.groupby([\"Customer_Gender\", \"Churn\"]).size().reset_index(name= \"Quantity\")\n",
        "\n",
        "# Calcula o total de clientes por forma de pagamento\n",
        "data_gender[\"Total\"] = data_gender.groupby(\"Customer_Gender\")[\"Quantity\"].transform('sum')\n",
        "\n",
        "# Compute the churn rate for each payment method\n",
        "data_gender[\"Percentage\"] = (data_gender[\"Quantity\"] / data_gender[\"Total\"]) * 100\n",
        "\n",
        "\n",
        "fig = px.bar(data_gender,\n",
        "             x=\"Customer_Gender\",\n",
        "             y=\"Quantity\",\n",
        "             color=\"Churn\",\n",
        "             barmode=\"group\",\n",
        "             text=\"Percentage\",\n",
        "             title=\"Percent Churn by Gender\",\n",
        "             color_discrete_map={\n",
        "                \"Yes\": \"Green\",\n",
        "                \"No\": \"Red\"\n",
        "             })\n",
        "\n",
        "fig.update_traces(texttemplate= \"%{text:.2f}%\")\n",
        "fig.update_layout(\n",
        "    width=1000,\n",
        "    height=600,\n",
        "    title_x=0.5,\n",
        "    yaxis_title= \"Quantity\",\n",
        "    font_family= \"Arial\",\n",
        "    title_font_color= \"black\",\n",
        "    xaxis_title= \"Gender\",\n",
        "    font_size=14\n",
        ")"
      ],
