    {
      "cell_type": "code",
      "source": [
        "# Group by payment method and cancellation status\n",
        "data_group = data.groupby([\"Customer_SeniorCitizen\", \"Churn\"]).size().reset_index(name= \"Quantity\")\n",
        "\n",
        "# Count customers by payment method\n",
        "data_group[\"Total\"] = data_group.groupby(\"Customer_SeniorCitizen\")[\"Quantity\"].transform(\"sum\")\n",
        "\n",
        "# Compute the churn rate for each payment method\n",
        "data_group[\"Percentage\"] = (data_group[\"Quantity\"] / data_group[\"Total\"]) * 100\n",
        "\n",
        "data_group"
      ],
